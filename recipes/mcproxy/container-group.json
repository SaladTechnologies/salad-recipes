{
  "autostartPolicy": true,
  "container": {
    "environmentVariables": {
      "MAX_CONTEXTS": "10",
      "CONTEXT_TTL_MS": "1800000"
    },
    "image": "ghcr.io/saladtechnologies/mcproxy/browser-server:latest",
    "imageCaching": true,
    "priority": "high",
    "resources": {
      "cpu": 1,
      "memory": 4096,
      "gpuClasses": [],
      "storageAmount": 5368709120,
      "shmSize": 1024
    }
  },
  "name": "",
  "networking": {
    "auth": false,
    "clientRequestTimeout": 100000,
    "loadBalancer": "least_number_of_connections",
    "port": 3000,
    "protocol": "http",
    "serverResponseTimeout": 100000,
    "singleConnectionLimit": false
  },
  "replicas": 3,
  "restartPolicy": "always",
  "readme": "$replace",
  "readinessProbe": {
    "failureThreshold": 1,
    "http": {
      "headers": [],
      "path": "/ready",
      "port": 8080,
      "scheme": "http"
    },
    "initialDelaySeconds": 0,
    "periodSeconds": 5,
    "successThreshold": 1,
    "timeoutSeconds": 5
  }
}
