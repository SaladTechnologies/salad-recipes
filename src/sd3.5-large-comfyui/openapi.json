    "/prompt": {
      "post": {
        "summary": "Submit Prompt",
        "description": "Submit an API-formatted ComfyUI prompt.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "prompt": {
                    "type": "object",
                    "additionalProperties": {
                      "type": "object",
                      "properties": {
                        "inputs": {},
                        "class_type": {
                          "type": "string"
                        },
                        "_meta": {}
                      },
                      "required": [
                        "class_type"
                      ],
                      "additionalProperties": false
                    }
                  },
                  "id": {
                    "type": "string",
                    "default": "50bc25b1-08e6-4cad-b0fe-c6dcca752389"
                  },
                  "webhook": {
                    "type": "string"
                  }
                },
                "required": [
                  "prompt"
                ],
                "additionalProperties": false
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "prompt": {
                      "type": "object",
                      "additionalProperties": {
                        "type": "object",
                        "properties": {
                          "inputs": {},
                          "class_type": {
                            "type": "string"
                          },
                          "_meta": {}
                        },
                        "required": [
                          "class_type"
                        ],
                        "additionalProperties": false
                      }
                    },
                    "images": {
                      "type": "array",
                      "items": {
                        "type": "string",
                        "contentEncoding": "base64"
                      }
                    },
                    "webhook": {
                      "type": "string"
                    },
                    "status": {
                      "type": "string",
                      "enum": [
                        "ok"
                      ]
                    }
                  },
                  "required": [
                    "id",
                    "prompt"
                  ],
                  "additionalProperties": false
                }
              }
            }
          },
          "202": {
            "description": "Default Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
