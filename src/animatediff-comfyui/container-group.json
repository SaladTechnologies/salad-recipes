{
  "name": "animatediff-comfyui",
  "display_name": "animatediff-comfyui",
  "container": {
    "image": "saladtechnologies/comfyui:comfy0.3.12-api1.8.1-animatediff-sprite-maker",
    "resources": {
      "cpu": 4,
      "memory": 24576,
      "gpu_classes": [
        "ed563892-aacd-40f5-80b7-90c9be6c759b"
      ]
    },
    "command": [],
    "priority": "high"
  },
  "autostart_policy": true,
  "restart_policy": "always",
  "replicas": 3,
  "networking": {
    "protocol": "http",
    "port": 3000,
    "auth": false,
    "load_balancer": "least_number_of_connections",
    "single_connection_limit": false,
    "client_request_timeout": 100000,
    "server_response_timeout": 100000
  },
  "readiness_probe": {
    "http": {
      "path": "/ready",
      "port": 3000,
      "scheme": "http",
      "headers": []
    },
    "initial_delay_seconds": 0,
    "period_seconds": 1,
    "timeout_seconds": 1,
    "success_threshold": 1,
    "failure_threshold": 3
  },
  "startup_probe": {
    "http": {
      "path": "/health",
      "port": 3000,
      "scheme": "http",
      "headers": []
    },
    "initial_delay_seconds": 6,
    "period_seconds": 2,
    "timeout_seconds": 10,
    "success_threshold": 1,
    "failure_threshold": 100
  }
}